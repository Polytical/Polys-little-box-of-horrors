<link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.7.2/leaflet.css" />
 <style type="text/css">

		.leaflet-tile-pane {
		  opacity: .5
		}
		.leaflet-container {
          background-color: #fff;
        }

    </style>
